    <script src="~/Scripts/jquery-3.4.1.min.js" type="text/javascript"></script>
</head>
    <body onload="PopulateDropDownList()">
        <div class="container">
            <div class="container-login">
                <div class="frontbox">
                    <div class="register">
                        <h2>REGISTER</h2>
                        <div class="inputbox">

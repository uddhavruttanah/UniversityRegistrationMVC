                                <input type="text" id="GuardianName" name="GuardianName" placeholder="Guardian Name">
                            </div>
                            <div class="contact">
                                <h3>CONTACT DETAILS</h3>
                                <input type="text" id="PhoneNo" name="PhoneNo" placeholder="Phone No">
                                <input type="text" id="Address" name="Address" placeholder="address">
                            </div>
                            <select id="SubjectList1">
                                <option value="" disabled selected hidden>HSC SUBJECT</option>
                            </select>
                            <select id="Grades1">
                                <option value="" disabled selected hidden>GRADES</option>
                                <option value="10">A</option>
                                <option value="8">B</option>
                                <option value="6">C</option>
                                <option value="4">D</option>
                                <option value="2">E</option>
                                <option value="0">F</option>
                            </select>
                            <br>
                            <select id="SubjectList2">
                                <option value="" disabled selected hidden>HSC SUBJECT</option>
                            </select>
                            <select id="Grades2">
                                <option value="" disabled selected hidden>GRADES</option>
                                <option value="10">A</option>
                                <option value="8">B</option>
                                <option value="6">C</option>
                                <option value="4">D</option>
                                <option value="2">E</option>
                                <option value="0">F</option>
                            </select>
                            <br>
                            <select id="SubjectList3">
                                <option value="" disabled selected hidden>HSC SUBJECT</option>
                            </select>
                            <select id="Grades3">
                                <option value="" disabled selected hidden>GRADES</option>
                                <option value="10">A</option>
                                <option value="8">B</option>
                                <option value="6">C</option>
                                <option value="4">D</option>
                                <option value="2">E</option>
                                <option value="0">F</option>
                            </select>
                        </div>
                            <div>
                            </div>
                        </div>
                        <button onclick="Register()">REGISTER</button>
                    </div>
                </div>
            </div>

    </body>
</html>

